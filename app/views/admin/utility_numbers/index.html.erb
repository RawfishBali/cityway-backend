<div class="md-card uk-margin-medium-bottom">
  <div class="md-card-content">
    <div class="uk-overflow-container">
      <table class="uk-table uk-table-nowrap table_check">
        <thead>
          <tr>
            <th class="uk-width-1-10">Name</th>
            <th class="uk-width-1-10 uk-text-center">Local Number</th>
            <th class="uk-width-1-10 uk-text-center">National Number</th>
            <th class="uk-width-1-10 uk-text-center">Address</th>
            <th class="uk-width-1-10 uk-text-center">Actions</th>
          </tr>
        </thead>
        <tbody>
          <% @admin_utility_numbers.each do  |utility_number|%>
          <tr>
            <td class=""><%= utility_number.name  %></td>
            <td class="uk-text-center"><%= utility_number.local_number %></td>
            <td class="uk-text-center"><%= utility_number.national_number %></td>
            <td class="uk-text-center"><%= utility_number.address %></td>
            <td class="uk-text-center">
              <%= link_to '<i class="md-icon material-icons">&#xE254;</i>'.html_safe , edit_admin_utility_number_path(utility_number)%>
              <%= link_to '<i class="md-icon material-icons ">&#xE872;</i>'.html_safe, admin_utility_number_path(utility_number), method: :delete , :data => {:confirm => 'Sei sicuro?'} %>
            </td>
          </tr>
          <% end %>
        </tbody>
      </table>
      <%= paginate @admin_utility_numbers %>
    </div>
  </div>
  <div class="md-fab-wrapper">
    <a class="md-fab md-fab-accent md-fab-wave-light waves-effect waves-button waves-light" href="/admin/utility_numbers/new" id="invoice_add">
      <i class="material-icons">î……</i>
    </a>
  </div>
</div>
