<div class="nested-fields">
  <div class="uk-grid" data-uk-grid-margin>
    <div class="uk-width-large-1-1 uk-width-1-1">
      <div class="uk-input-group subcategory-container">

        <div class="uk-grid" data-uk-grid-margin="">
          <div class="uk-width-medium-1-1 uk-row-first">

            <div class="md-input-wrapper md-input-filled"><label>Data d'inizio</label>
              <%= f.text_field :start_date, class: 'md-input', value:(f.object.start_date ?  f.object.start_date.strftime("%d/%m/%Y") : ''), 'data-uk-datepicker':"{format:'DD/MM/YYYY'}", required: true %>
              <span class="md-input-bar "></span></div>

          </div>
          <div class="uk-width-medium-1-1 uk-row-first">

            <div class="md-input-wrapper md-input-filled"><label>Data fine</label>
              <%= f.text_field :end_date, class: 'md-input', value:(f.object.end_date ?  f.object.end_date.strftime("%d/%m/%Y") : ''), 'data-uk-datepicker':"{format:'DD/MM/YYYY'}", required: true %>
              <span class="md-input-bar "></span></div>

          </div>
        </div>
        <div class="uk-grid" data-uk-grid-margin="">
          <div class="uk-width-medium-1-2 uk-row-first">
            <div class="md-input-wrapper">
            </div>
          </div>
          <div class="uk-width-medium-1-2 uk-row-first">
            <div class="md-input-wrapper text-align-right">
              <%= link_to_remove_association '<i class="material-icons md-24 ">î¡²</i>'.html_safe, f, {class: 'btnSectionRemove'} %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
