<h1 class: 'center'> Choose A City </h1>
<div class='uk-grid uk-grid-width-small-1-2 uk-grid-width-large-1-3 uk-grid-width-xlarge-1-5'>
  <% @cities.each do |city| %>
  <div>
    <div class="md-card md-card-hover md-card-overlay marginB25">
      <div class="md-card-content uk-flex uk-flex-center uk-flex-middle md-content-margin">
        <%= image_tag city.photo.url , class: '100p' %>
      </div>
      <div class="md-card-overlay-content">
        <div class="uk-clearfix md-card-overlay-header">

          <h3>
            <%= link_to city.name, edit_admin_city_path(city)  %>
          </h3>
        </div>
        Lorem ipsum dolor sit amet, consectetur adipisicing elit.
      </div>
    </div>
  </div>
  <% end %>
</div>

<div class="md-fab-wrapper">
  <a class="md-fab md-fab-accent md-fab-wave-light waves-effect waves-button waves-light" href="/admin/cities/new" id="invoice_add">
    <i class="material-icons">î……</i>
  </a>
</div>
